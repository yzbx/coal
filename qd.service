[Unit]
Description=QD Service
After=network.target

[Service]
Type=simple
User=dell
WorkingDirectory=/home/dell
Environment="PORT=8015"
ExecStart=/usr/bin/docker run --runtime nvidia -d -v ~/iscas:/workspace -p ${PORT}:${PORT} youdaoyzbx/pytorch bash -c "cd /workspace/qd && python tasks.py -p ${PORT}"
